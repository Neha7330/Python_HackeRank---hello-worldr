# hackerRank :-

def is_leap(year):
    leap = False
    
    if year % 4 == 0 and year % 100 != 0 or year % 400 ==0:
        leap = True
    
    return leap

year = int(input())


# leap year code:-

def is_leap(year):
    leap = False
    
    if year % 4 == 0 and year % 100 != 0 or year % 400 ==0:
        leap = True
    
    return leap
    
year_input = int(input("Enter the year :- "))
ans = is_leap(year_input)
if ans == True:
    print("leap year")
else:
    print("Not a leap year")
